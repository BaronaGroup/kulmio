{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "target": "es2020",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "outDir": "build",
    "incremental": true
  },
  "include": ["src", "generated"]
}
